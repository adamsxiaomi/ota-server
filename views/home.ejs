<body>
    <!--Body content-->

<form class="form-horizontal" role="form">
    <div class="form-group">
        <label for="version" class="col-sm-2 control-label">版本号</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="version" placeholder="版本号">
        </div>
    </div>
    <div class="form-group">
        <label for="devType" class="col-sm-2 control-label">设备类型</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="devType" placeholder="设备类型">
        </div>
    </div>

</form>






<div dir=rtl class="file-loading"> <!-- note the direction if you want to display file-loading indicator -->
                <!-- note that your input must just set the `rtl` data property of the plugin or in your javascript code -->
                <input id="input-b8" name="input-b8[]" multiple type="file"'>
</div>
<script>
                $(document).ready(function() {
                    $("#input-b8").fileinput({
                        rtl: true,
                        dropZoneEnabled: false,
                        allowedFileExtensions: ["bin"],
                        uploadUrl: '/upload',
                        typ: 'POST',
                        enctype: 'multipart/form-data'
                        
                    });
                });
</script>
</body>


<div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
        <button type="btn_upload" class="btn btn-default">升级</button>
    </div>
</div>





</html>
